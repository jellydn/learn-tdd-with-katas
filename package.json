{
  "name": "tdd-demo",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "rome": "rome",
    "rome:check": "rome check --apply-suggested {apps,packages}/**/*.{ts,tsx,md}",
    "rome:format": "rome format --write --skip-errors {apps,packages}/**/*.{ts,tsx,md}",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@total-typescript/shoehorn": "0.1.0",
    "eslint": "8.41.0",
    "rome": "12.1.2",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.29.2",
  "engines": {
    "node": ">=14.0.0"
  }
}
