{
  "name": "db",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "lib": "tsup",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "dependencies": {
    "pg": "8.16.0",
    "reflect-metadata": "0.2.2",
    "typeorm": "0.3.24"
  },
  "devDependencies": {
    "@swc/core": "1.11.31",
    "@types/node": "22.15.31",
    "ts-node": "10.9.2",
    "tsconfig": "workspace:*",
    "tsup": "8.5.0",
    "typescript": "5.8.3"
  },
  "tsup": {
    "entry": [
      "src/**/*.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "dts": true,
    "format": [
      "cjs"
    ]
  }
}
